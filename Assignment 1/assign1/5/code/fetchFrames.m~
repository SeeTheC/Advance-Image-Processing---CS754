% Fetches the T video from of dim HxW
function [ output_args ] = fetchFrames( )
    %If it is a possitive number then it should always be accurate
    nFrames=video.nrFramesTotal; 
    H=video.height;
    W=video.width;
    frameTotal=vid1.frames;

    T=3; %no.of frames
    frame=zeros(120,240,T);
    for i=1:T    
        img=rgb2gray(frameTotal(i).cdata);    
        frame(:,:,i)=img(H-120+1:H,W-240+1:W);
    end
    H=120;W=240;

end

