function [thetaFinal,support]=omp(y,A,eps)
    support=[];
    i=1;r=y;
    row=size(y,1);
    % unit normalizing A
    normACol=sqrt(sum(A.^2));
    uA=bsxfun(@times,A,normACol.^-1);
    
    while(norm(r)>eps)        
        a=r'*uA;
        [aMax, index]=max(a);
        support=[support,index];
        i=i+1;
        Ati=A(:,support);
        theta=Ati\y;
        theta1=inv(Ati'*Ati)*Ati'*y;
        r=y-Ati*theta;
    end
%    disp(aMax)
    thetaFinal=zeros(64*3,1);
    thetaFinal(support)=theta;
end